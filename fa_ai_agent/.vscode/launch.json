    "version": "0.2.0",
    "configurations": [
        {
            "name": "fa_ai_agent (staging)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port=3001",
                "--dart-define=FLUTTER_ENV_STAGING=true"
            ]
        },
        {
            "name": "fa_ai_agent (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--web-port=3001",
                "--dart-define=FLUTTER_ENV_STAGING=true"
            ]
        },
        {
            "name": "fa_ai_agent (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--web-port=3001",
                "--dart-define=FLUTTER_ENV_PRODUCTION=true"
            ]
        }
    ]
}